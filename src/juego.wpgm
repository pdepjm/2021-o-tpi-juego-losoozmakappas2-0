import wollok.game.*
import configuraciones.*
import niveles.*

program juego {
    game.title("Pokemon")
    game.width(15)
    game.height(10)
    nivel.configurarElementos()
    
    const niveles = [nivelBosque,nivelElectrico]
    
    
    nivel.cargarNivel(actual.nivel())
    nivel.configurarTeclas()
    
    game.start()
}